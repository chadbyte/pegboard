{
  "name": "pegboard",
  "version": "0.1.0",
  "description": "Pegboard - Multi-agent AI workspace on an infinite canvas",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "electron:build": "npm run build && electron-builder",
    "preview": "vite preview"
  },
  "keywords": [],
  "author": "chadbyte",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^7.1.12",
    "wait-on": "^9.0.1"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.41",
    "@ai-sdk/google": "^2.0.28",
    "@ai-sdk/openai": "^2.0.62",
    "@google/genai": "^1.34.0",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@xenova/transformers": "^2.17.2",
    "ai": "^6.0.73",
    "lucide-react": "^0.552.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7"
  },
  "build": {
    "appId": "com.pegboard.app",
    "productName": "Pegboard",
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        {
          "target": "zip",
          "arch": ["arm64"]
        }
      ],
      "icon": "assets/icon.icns",
      "identity": null
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}
